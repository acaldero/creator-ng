{
  "comments": {
    "lineComment": "#",
    "blockComment": ["/*", "*/"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*[^#]*:.*$",
    "decreaseIndentPattern": "^\\s*$"
  },
  "folding": {
    "markers": {
      "start": "^\\s*[^#]*:.*$",
      "end": "^\\s*$"
    }
  }
}
